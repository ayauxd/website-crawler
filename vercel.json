{
  "version": 2,
  "builds": [
    {
      "src": "web_interface_retro.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "web_interface_retro.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production",
    "FLASK_APP": "web_interface_retro.py"
  }
} 